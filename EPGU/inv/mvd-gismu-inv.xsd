<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="urn://mvd/gismu/inv/root/1.0.0"
           elementFormDefault="qualified"
           xmlns:ns0="urn://mvd/gismu/inv/root/1.0.0"
           xmlns:smev="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.2"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:ns2="urn://ibd-m/common/0.2.1"

>
    <xs:import namespace="urn://ibd-m/common/0.2.1" schemaLocation="commons/common-0.2.1.xsd"/>
    <xs:import namespace="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.2"
               schemaLocation="commons/smev-supplementary-commons-1.2.xsd"/>

    <xs:annotation>
        <xs:documentation>Вид сведений по оформлению и выдаче приглашений на въезд в РФ иностранных граждан и лиц без
            гражданства.
        </xs:documentation>
    </xs:annotation>

    <xs:element name="EPGURequest" type="ns0:EPGURequestType">
        <xs:annotation>
            <xs:documentation>Базовый запрос</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="EPGUResponse" type="ns0:EPGUResponseType">
        <xs:annotation>
            <xs:documentation>Базовый ответ</xs:documentation>
        </xs:annotation>
    </xs:element>

    <!-- Поля и типы запроса -->

    <xs:complexType name="EPGURequestType">
        <xs:annotation>
            <xs:documentation>Базовый запрос</xs:documentation>
        </xs:annotation>

        <xs:choice>
            <xs:element name="formData" type="ns0:FormDataType">
                <xs:annotation>
                    <xs:documentation>Передача заявления от заявителя в ведомство</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cancelRequest" type="ns0:CancelRequestType">
                <xs:annotation>
                    <xs:documentation>Запрос на отмену запроса на оказание государственной услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="textRequest" type="ns0:TextRequestType">
                <xs:annotation>
                    <xs:documentation>Текстовое сообщение от заявителя в ведомство</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>

        <xs:attribute name="env" type="ns0:EnvType" use="optional">
            <xs:annotation>
                <xs:documentation>Среда вызова</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Id" type="smev:string-50" use="optional">
            <xs:annotation>
                <xs:documentation>Идентификатор элемента</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="FormDataType">
        <xs:sequence>
            <xs:element name="orderId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>Номер заявления на ЕПГУ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="serviceCode" type="smev:string-50">
                <xs:annotation>
                    <xs:documentation>Код услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="procedureCode" type="smev:string-50">
                <xs:annotation>
                    <xs:documentation>Код процедуры</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="user_type" type="smev:string-50">
                <xs:annotation>
                    <xs:documentation>Тип заявителя</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:choice>
                <xs:element name="byArtificialPerson" type="ns0:ByArtificialPerson">
                    <xs:annotation>
                        <xs:documentation>Приглашающей стороной выступает юридическое лицо</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="byPhysicalPerson" type="ns0:ByPhysicalPerson">
                    <xs:annotation>
                        <xs:documentation>Приглашающей стороной выступает физическое лицо</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="validity" type="ns0:ValidType">
                    <xs:annotation>
                        <xs:documentation>Проверка действительности приглашения</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="readiness" type="ns0:ReadinessType">
                    <xs:annotation>
                        <xs:documentation>Проверка готовности приглашения</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>


    <!--[Запросы][Основной] Юрлицо -->
    <xs:complexType name="ByArtificialPerson">
        <xs:annotation>
            <xs:documentation>Приглашающей стороной выступает юридическое лицо</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Application" type="ns0:LegalApplicationType">
                <xs:annotation>
                    <xs:documentation>Запрос (приглашающей стороной выступает юридическое лицо) на предоставление услуги
                        по оформлению и выдаче приглашений на въезд в РФ иностранных граждан и лиц без гражданства
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LegalApplicationType">
        <xs:annotation>
            <xs:documentation>Запрос (приглашающей стороной выступает юридическое лицо) на предоставление
                государственной услуги
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="isFormaliseBlank" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Способ получения (В форме электронного документа/ на формализованном бланке)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="applicationRegion" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Регион подачи заявления Автоматически из региона юридического адреса. Справочник "Регионы РФ"
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="applicantLegalPerson" type="ns0:LegalApplicantType">
                <xs:annotation>
                    <xs:documentation>Сведения о приглашающей организации</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="urgentRegistration" type="ns2:DateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Желательно оформить в срок до. Формат даты ДД.ММ.ГГГГ Основания для срочного
                        оформления прилагаются
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="invitedApplicant" type="ns0:InvitedApplicantType">
                <xs:annotation>
                    <xs:documentation>Сведения о целях поездки, приглашаемом лице и сопровождаемых (при наличии)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AppliedDocuments" type="ns0:AppliedDocumentsType">
                <xs:annotation>
                    <xs:documentation>Сведения о документах, прилагаемых к заявителю</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="LegalApplicantType">
        <xs:annotation>
            <xs:documentation>Сведения о приглашающей организации.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- УД организации -->
            <xs:element name="fullName" type="smev:string-500">
                <xs:annotation>
                    <xs:documentation>Полное наименование организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="shortName" type="smev:string-200">
                <xs:annotation>
                    <xs:documentation>Краткое наименование организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inn" type="smev:LegalPersonINNType">
                <xs:annotation>
                    <xs:documentation>ИНН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ogrn" type="smev:OGRNCompanyType">
                <xs:annotation>
                    <xs:documentation>ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>

            <!-- Информация об аккредитации-->
            <xs:element name="accreditationNumber" type="smev:string-50" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер записи об аккредитации (для филиалов, представительств)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="accreditationDate" type="ns2:DateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата внесения записи об аккредитации. В формате ДД.ММ.ГГГГ</xs:documentation>
                </xs:annotation>
            </xs:element>

            <!-- Контактная информация -->
            <xs:element name="phoneNumber" type="smev:PhoneNumberRFType">
                <xs:annotation>
                    <xs:documentation>Контактный телефон с кодом города. В формате +7(XXX)XXX-XX-XX</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="additionalNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Добавочный номер к телефону, если имеется</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="email" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Адрес электронной почты</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="legalPersonAddress" type="ns2:FIASAddressType">
                <xs:annotation>
                    <xs:documentation>Юридический адрес</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="actualAddress" type="ns2:FIASAddressType">
                <xs:annotation>
                    <xs:documentation>Фактический (почтовый) адрес</xs:documentation>
                </xs:annotation>
            </xs:element>

            <!-- Ответственное лицо -->
            <xs:element name="official" type="ns0:OfficialInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения об уполномоченном лице организации, подтверждающем достоверность
                        указанных в ходатайстве сведений на приглашаемое лицо и лиц, следующих с ним совместно.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="OfficialInfoType">
        <xs:annotation>
            <xs:documentation>Сведения об уполномоченном лице организации</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="appoitment" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Должность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="surname" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Фамилия</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Имя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="patronymic" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Отчество</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="birthDate" type="ns2:DateType">
                <xs:annotation>
                    <xs:documentation>Дата рождения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="citizenship" type="xs:string"  minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Гражданство. Справочник "Страны"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="officialAddressFIAS" type="ns2:FIASAddressType">
                <xs:annotation>
                    <xs:documentation>Адрес проживания/ пребывания ФИАС (если гражданство Россия)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="passportId" type="ns0:PassportIDType">
                <xs:annotation>
                    <xs:documentation>Сведения о документе, удостоверяющем личность</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>


    <!--[Запросы][Основной] Физлицо -->
    <xs:complexType name="ByPhysicalPerson">
        <xs:annotation>
            <xs:documentation>Приглашающей стороной выступает физическое лицо</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Application" type="ns0:PersonApplicationType">
                <xs:annotation>
                    <xs:documentation>Запрос (приглашающей стороной выступает физическое лицо) на предоставление услуги
                        по оформлению и выдаче приглашений на въезд в РФ иностранных граждан и лиц без гражданства
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PersonApplicationType">
        <xs:annotation>
            <xs:documentation>Запрос (приглашающей стороной выступает физическое лицо) на предоставление государственной
                услуги
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="isFormaliseBlank" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Способ получения (В форме электронного документа/ на формализованном бланке)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="applicationRegion" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Регион подачи заявления. Справочник "Регионы РФ"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="applicantPhysicalPerson" type="ns0:PersonApplicantType">
                <xs:annotation>
                    <xs:documentation>Сведения о приглашающем лице</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="urgentRegistration" type="ns2:DateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Желательно оформить в срок до. Формат даты ДД.ММ.ГГГГ Основания для срочного
                        оформления прилагаются
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="invitedApplicant" type="ns0:InvitedApplicantType"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения о целях поездки, приглашаемых лицах и сопровождаемых (при наличии)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AppliedDocuments" type="ns0:AppliedDocumentsType">
                <xs:annotation>
                    <xs:documentation>Сведения о документах, прилагаемых к заявителю</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PersonApplicantType">
        <xs:annotation>
            <xs:documentation>Сведения о приглашающем лице</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FIO" type="ns2:NameType">
                <xs:annotation>
                    <xs:documentation>Фамилия, имя, отчество</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="gender" type="smev:GenderType">
                <xs:annotation>
                    <xs:documentation>Пол. Муж/Жен</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="birthDate" type="ns2:DateType">
                <xs:annotation>
                    <xs:documentation>Дата рождения. В формате ДД.ММ.ГГГГ (год полностью)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="stateOfBirth" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Государство рождения. Справочник "Страны"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="placeOfBirth" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Точное место рождения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="passportID" type="ns0:PassportIDType">
                <xs:annotation>
                    <xs:documentation>Сведения о документе, удостоверяющем личность</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="citizenship" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Гражданство (подданство). Справочник "Страны"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="residencePlace" type="ns2:FIASAddressType">
                <xs:annotation>
                    <xs:documentation>Место регистрации</xs:documentation>
                </xs:annotation>
            </xs:element>



            <!-- Данные для ИП -->
            <xs:element form="qualified"  minOccurs="0" name="OGRNSoleProprietorType"
                        type="smev:OGRNSoleProprietorType">
                <xs:annotation>
                    <xs:documentation>ОГРН индивидуального предпринимателя</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="inn" type="smev:PhysicalPersonINNType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>ИНН физического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SNILS" type="smev:SNILSType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>СНИЛС</xs:documentation>
                </xs:annotation>
            </xs:element>




            <!-- Контактная информация-->
            <xs:element name="phoneNumber" type="smev:PhoneNumberRFType">
                <xs:annotation>
                    <xs:documentation>Домашний телефон</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="email" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Адрес электронной почты</xs:documentation>
                </xs:annotation>
            </xs:element>

            <!-- Трудовая деятельность -->
            <xs:element name="employmentList" type="ns0:EmploymentListType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сведения о деятельности за последние 5 лет</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>




    <!--[Запросы][Основной] Общее -->

    <xs:complexType name="InvitedApplicantType">
        <xs:sequence>
            <xs:element name="tripInformation" type="ns0:TripInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения о поездке</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="invitedApplicantInfo" type="ns0:InvitedApplicantInfoType">
                <xs:annotation>
                    <xs:documentation>Сведения о приглашаемом лице</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="companionInfo" type="ns0:CompanionInfoType" minOccurs="0"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения о сопровождаемых лицах</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TripInfoType">
        <xs:annotation>
            <xs:documentation>Сведения о поездке
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="purposeOfVisit" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Цель поездки из Справочник "Цель поездки"
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="periodDays" type="xs:string">
                <xs:annotation>
                    <xs:documentation>На срок (дней).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entranceDate" type="ns2:DateType">
                <xs:annotation>
                    <xs:documentation>Предполагаемая дата въезда в РФ. Дата в формате ДД.ММ.ГГ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="departureDate" type="ns2:DateType">
                <xs:annotation>
                    <xs:documentation>Предполагаемая дата убытия из РФ. Дата в формате ДД.ММ.ГГ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numberOfEntries" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Кратность визы</xs:documentation>
                    <xs:documentation>Справочник "Кратность визы"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="visa" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Вид визы</xs:documentation>
                    <xs:documentation>Справочник "Категория визы"</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="visitPoints" type="ns0:VisitPointsType" >
                <xs:annotation>
                    <xs:documentation>Пункты посещения в России</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="permissionForEntrance" type="ns0:PermissionForEntranceType"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Специальное разрешение (согласование) для въезда на территорию с
                        регламентированным посещением
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="VisitPointsType">
        <xs:annotation>
            <xs:documentation>Пункты посещения в России</xs:documentation>
        </xs:annotation>
        <xs:sequence >
            <xs:element maxOccurs="unbounded" name="visitPoint" type="ns2:FIASAddressType">
                <xs:annotation>
                    <xs:documentation>Пункт посещения в России. Адрес по ФИАС ло населенного пкнкта (регион, насел.
                        пункт)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PermissionForEntranceType">
        <xs:annotation>
            <xs:documentation>Специальное разрешение (согласование) для въезда на территорию с регламентированным
                посещением
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element name="issuedBy" type="xs:string" >
                <xs:annotation>
                    <xs:documentation>Наименование органа, выдавшего разрешение</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="number" type="xs:string" >
                <xs:annotation>
                    <xs:documentation>Номер</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="dateFrom" type="ns2:DateType" >
                <xs:annotation>
                    <xs:documentation>От. Дата в формате ДД.ММ.ГГ.</xs:documentation>
